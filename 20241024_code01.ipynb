    "def solution(a, b, n):\n",
    "    answer = 0\n",
    "    while n >= a: \n",
    "        exchanged = (n//a)*b\n",
    "        answer += exchanged\n",
    "        n = (n % a) + exchanged\n",
    "    return answer\n",
    "\n",
    "a = 2\n",
    "b = 1 \n",
    "n = 20 \n",
    "print(solution(a, b, n))"
   ]
  }
